import java.util.Scanner;
public class {

public static void main (String[] args) {
Scanner input = new Scanner (System.in); 

System.out.println("Enter A Number: ");
int x = input.nextInt();

System.out.println("Enter Another Number: ");
int y = input.nextInt();

int sum;
sum = x + y;

System.out.println("The SUM of 2 numbers is: " + sum);

int diff;
diff = x - y;

System.out.println("The DIFF of 2 numbers is: " + diff);

int prod;
prod = x * y;

System.out.println("The PROD of 2 numbers is: " + prod);

int quot;
quot = x / y;

System.out.println("The QUOT of 2 numbers is: " + quot);

}
}
